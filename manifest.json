{
    "manifest_version": 3,
    "name": "Proxy Manager",
    "version": "1.0",
    "description": "Manage your proxy servers",
    "permissions": [
      "proxy",
      "storage",
      "webRequest",
      "webRequestAuthProvider",
      "contextMenus"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon-128.png"
      }
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "icons": {
      "128": "icon-128.png"
    }
  }